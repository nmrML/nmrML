        <oboInOwl:hasExactSynonym rdf:datatype="&xsd;string">NMR pulse sequence</oboInOwl:hasExactSynonym>
        <oboInOwl:created_by rdf:datatype="&xsd;string">burchh</oboInOwl:created_by>
        <oboInOwl:hasExactSynonym rdf:datatype="&xsd;string">pulse sequence</oboInOwl:hasExactSynonym>
        <obo:IAO_0000412 rdf:resource="http://rsc-cmo.googlecode.com/svn/trunk/chmo.owl"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CHMO_0001843 -->

    <owl:Class rdf:about="&obo;CHMO_0001843">
        <rdfs:label rdf:datatype="&xsd;string">bilinear rotation decoupling pulse sequence</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;CHMO_0001841"/>
