        <obo:IAO_0000115 rdf:datatype="&xsd;string">A pulse cluster (a spin echo on 1H with inversion of 13C in the centre) used to achieve selective inversion of 12C-bound protons, thus achieving the removal of signals due to 12C-bound nuclei. For the pulse sequence see Chem. Phys. Lett., (1982), 93, pg 504</obo:IAO_0000115>
        <oboInOwl:hasExactSynonym rdf:datatype="&xsd;string">BIRD</oboInOwl:hasExactSynonym>
        <oboInOwl:id rdf:datatype="&xsd;string">CHMO:0001843</oboInOwl:id>
        <oboInOwl:hasExactSynonym rdf:datatype="&xsd;string">bilinear rotation decoupling</oboInOwl:hasExactSynonym>
        <oboInOwl:created_by rdf:datatype="&xsd;string">burchh</oboInOwl:created_by>
        <obo:IAO_0000412 rdf:resource="http://rsc-cmo.googlecode.com/svn/trunk/chmo.owl"/>
    </owl:Class>
    


