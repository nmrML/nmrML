        <obo:IAO_0000412 rdf:resource="http://rsc-cmo.googlecode.com/svn/trunk/chmo.owl"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CHMO_0001841 -->

    <owl:Class rdf:about="&obo;CHMO_0001841">
