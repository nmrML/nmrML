            <owl:Restriction>
                <owl:onProperty rdf:resource="&btl2;hasGranularPart"/>
                <owl:allValuesFrom rdf:resource="&btl2;Organism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>Collection of two or more organisms of the same sort.  </skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/biotop/btl2.owl#PointInTime -->

    <owl:Class rdf:about="&btl2;PointInTime">
        <rdfs:label>point in time</rdfs:label>
        <rdfs:subClassOf rdf:resource="&btl2;TemporalRegion"/>
