                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/biotop/btl2.owl#PluralityOfOrganisms -->

    <owl:Class rdf:about="&btl2;PluralityOfOrganisms">
