        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&btl2;includes"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&btl2;Disposition"/>
                            <rdf:Description rdf:about="&btl2;Function"/>
                            <rdf:Description rdf:about="&btl2;ImmaterialObject"/>
                            <rdf:Description rdf:about="&btl2;InformationObject"/>
                            <rdf:Description rdf:about="&btl2;MaterialObject"/>
                            <rdf:Description rdf:about="&btl2;Process"/>
                            <rdf:Description rdf:about="&btl2;Quality"/>
                            <rdf:Description rdf:about="&btl2;Role"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
