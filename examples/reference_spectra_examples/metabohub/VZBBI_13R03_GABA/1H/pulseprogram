# 12 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg" 2


# 15 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg"
"acqt0=-p1*2/3.1416"


# 1 "mc_line 18 file /opt/topspin3.0/exp/stan/nmr/lists/pp/zg exp. def. part of mc cmd. before ze"
define delay MCWRK
define delay MCREST
"MCWRK = 0.333333*30m"
"MCREST = 30m - 30m"

    dccorr
# 18 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg"
1 ze
# 1 "mc_line 18 file /opt/topspin3.0/exp/stan/nmr/lists/pp/zg exp. def. of mc cmd. after ze"
# 19 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg"
# 1 "mc_line 19 file /opt/topspin3.0/exp/stan/nmr/lists/pp/zg exp. start label for mc cmd."
2 MCWRK  * 2
LBLF0, MCWRK
  MCREST
# 20 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg"
  d1
  p1 ph1
  go=2 ph31
# 1 "mc_line 23 file /opt/topspin3.0/exp/stan/nmr/lists/pp/zg exp. mc cmd. in line"
  MCWRK wr #0
  MCWRK zd
  lo to LBLF0 times td0

  MCWRK
# 24 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg"
exit
# 27 "/opt/topspin3.0/exp/stan/nmr/lists/pp/zg"
ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1


;pl1 : f1 channel - power level for pulse (default)
;p1 : f1 channel -  high power pulse
;d1 : relaxation delay; 1-5 * T1
;NS: 1 * n, total number of scans: NS * TD0
