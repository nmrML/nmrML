T_NAME		PLW1
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0 108
		REL	"PLW1=PLW[1]"
		INV_REL	"PLW[1]=PLW1"
		UNIT	"W"
		FORMAT	"%14.5g W"
TEXT		"f1 channel - power level for pulse (default)"
END
